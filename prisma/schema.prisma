generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(cuid())
  email        String  @unique
  name         String?
  password     String
  accessToken  String?
  refreshToken String?
}

model Blacklist {
  id           String   @id @default(cuid())
  accessToken  String
  refershToken String
  createdAt    DateTime @default(now())
}
